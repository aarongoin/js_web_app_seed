{
  "name": "js_web_app_seed",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "//": "See README for documentation about these commands.",
    "start": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "debug": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/nodemon --inspect server.js",
    "test": "./node_modules/.bin/cross-env NODE_ENV=test jest",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production webpack",
    "dev": "webpack -d --watch",
    "recreate_db": "rm -rf ./db/data ; initdb -D ./db/data && npm run postgres",
    "postgres": "postgres -D ./db/data",
    "setup_db": "createdb app_db && psql -f ./db/init.sql app_db",
    "set_admin": "node ./db/admin.js $*"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "basscss": "^8.0.4",
    "muicss": "^0.9.39",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.0",
    "body-parser": "1.17.1",
    "compression": "^1.7.2",
    "connect-pg-simple": "4.2.0",
    "cookie-parser": "1.4.3",
    "cross-env": "5.0.0",
    "debug": "~2.6.9",
    "eslint": "3.19.0",
    "express": "~4.16.0",
    "express-promise-router": "^3.0.2",
    "express-session": "^1.15.6",
    "express-validator": "3.2.0",
    "flow": "^0.2.3",
    "helmet": "^3.12.0",
    "http-errors": "~1.6.2",
    "jest": "^22.4.3",
    "method-override": "2.3.8",
    "morgan": "~1.9.0",
    "nodemon": "1.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "winston": "2.3.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/config/",
      "/node_modules/",
      "/private/"
    ]
  }
}
